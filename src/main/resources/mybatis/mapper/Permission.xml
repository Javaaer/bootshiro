<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.shirotest.bootshiro.mapper.PermissionMapper" >

    <select id="getPermissionById" resultType="com.shirotest.bootshiro.model.Permission"  >
        SELECT
            *
        FROM
            permission
        where id = #{id}
    </select>

</mapper>